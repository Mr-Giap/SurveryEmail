@using ValueObjects
@model ValueObjects.OUsers
@{ 
    var userName = (OUsers)HttpContext.Current.Session["SESSION_ACCOUNT"];   
}
@if (Session["SESSION_ACCOUNT"] != null)
{
    <li><a>@userName.FullName</a></li>
    <li><a href="@Url.Action("Logout","Account")">Logout</a></li>
}
else
{
    <li><a href="@Url.Action("Login","Account")">Login</a></li>
    <li><a href="@Url.Action("Register","Account")">Register</a></li>
}